    "@types/react-dom": "18.2.7",
    "flowbite": "^1.5.5",
    "flowbite-react": "^0.3.7",
    "next": "13.4.17",
    "next-auth": "^4.23.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
